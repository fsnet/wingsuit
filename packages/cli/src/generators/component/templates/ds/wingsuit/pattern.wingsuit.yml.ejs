<%= underscoreName %>:
  use: "@<%= cleanPatternType %>/<%= name %>/<%= name %>.twig"
  label: <%= capitalizeName %>
  description: Example description
<% if (hasVariants) { -%>
  variants:
<% variants.forEach(function(variant) { -%>
    <%= variant.name %>:
      label: <%= variant.label %>
<% }); -%>
<% } -%>
<% if (hasFields) { -%>
  fields:
<% fields.forEach(function(field) { -%>
    <%= field.name %>:
      type: <%= field.type %>
      label: <%= field.label %>
<% if (field.preview) { -%>
      preview:
        faker: lorem.word
<% } -%>
<% }); -%>
<% } -%>
<% if (hasSettings) { -%>
  settings:
<% settings.forEach(function(setting) { -%>
    <%= setting.name %>:
      type: <%= setting.type %>
      label: <%= setting.label %>
<% }); -%>
<% } -%>
<% if (useJs || useCss) { -%>
  libraries:
    - pattern_<%= underscoreName %>:
<% if (useCss || useScss) { -%>
       css:
         component:
           ../<%= name %>.css: {}
<% } -%>
<% if (useJs) { -%>
       js:
         ../behaviors/<%= name %>.js: {}
<% } -%>
<% } -%>
