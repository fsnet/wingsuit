    "@wingsuit-designsystem/cli": "2.0.0-alpha.0",
    "@wingsuit-designsystem/preset-postcss8": "2.0.0-alpha.0",
    "@wingsuit-designsystem/preset-scss": "2.0.0-alpha.0",
    "@wingsuit-designsystem/storybook": "2.0.0-alpha.0",
    "autoprefixer": "^9.7.4",
